{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "corejs": { "version": 3, "proposals": true }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-spread"
        ],
        [
            "@babel/plugin-transform-destructuring",
            {
                "useBuiltIns": true
            }
        ],
        [
            "@babel/plugin-proposal-export-default-from"
        ]
    ]
}